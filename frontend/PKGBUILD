pkgname="tagger-player"
pkgver="1.0.0"
pkgrel=1
pkgdesc="An Electron / Go app that ables you to tag your music files and make playlists out of them."
arch=("x86_64")
url="https://github.com/zekicaneksi/Tagger-Player"
license=('GPL')
groups=()
depends=("dpkg")
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
source=("tagger-player_1.0.0_amd64.deb")
sha256sums=("SKIP")

package() {
  dpkg --extract "${srcdir}/tagger-player_1.0.0_amd64.deb" "${srcdir}/"
  cp -r "${srcdir}/usr/" "${pkgdir}/"
  chmod +x "${pkgdir}/usr/bin/tagger-player"
}
